}
.metor_progess .stepper-item {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
/*	align-items: flex-start;*/
    flex: 1 1;
}
.metor_progess .stepper-item:after {
    position: absolute;
    content: "";
    border-bottom: 10px solid #f2f2f2;
    width: 100%;
    top: 20px;
    left: -50%;
/*	left: 0;*/
    z-index: 2;
}
.metor_progess .first:after {
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
}
.metor_progess .stepper-item.completed:after {
    position: absolute;
    content: "";
    border-bottom: 10px solid #000;
    width: 100%;
    top: 20px;
    left: 50%;
/*	left: 0;*/
    z-index: 3;
}
.metor_progess .stepper-item:before {
    position: absolute;
    content: "";
/*    border-bottom: 2px solid #f2f2f2;*/
    width: 100%;
    top: 20px;
    left: -50%;
    z-index: 2;
}
.metor_progess .step-image {
    top: 15%;
    width: 36px;
    height: 36px;
}
.metor_progess .progress-dot {
    width: 16px;
    height: 16px;
    background: #e44332;
    border: 1px solid #000;
    border-radius: 4px;
    transform: rotate(-45deg);
	position: absolute;
    top: 17px;
    z-index: 5;
}
.metor_progess .stepper-item img {
    max-width: 20px;
    position: relative;
    z-index: 9;
    top: 10px;
/*	left:-7px*/
}


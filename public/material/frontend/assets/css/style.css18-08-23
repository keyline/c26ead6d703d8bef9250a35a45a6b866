/*check box css*/

.metor_information .button-group {
  display: table;
}
.metor_information .button-group__btn {
  cursor: pointer;
  display: block;
  position: relative;
  margin-bottom: 10px;	
}
.metor_information .button-group__btn input[type=radio],
.metor_information .button-group__btn input[type=checkbox] {
  opacity: 0;
  position: absolute;
}
.metor_information .button-group__label {
  background-color: transparent;
  border: 2px solid #ddd;
  color: #000;
  display: block;
  height: 50px;
  line-height: 50px;
  padding: 0 20px;
  text-align: left;
  border-radius: 5px;
	font-family: 'MyriadPro-Regular';
    font-size: 14px;
}

.metor_information input:checked + .button-group__label {
  background-color: #f7f7f7;
  border:2px solid #000;
  color: #000;
}
.metor_information .button-group__btn:first-child input:checked + .button-group__label {
  border-left-color: #666;
}
.metor_information .button-group__btn:last-child input:checked + .button-group__label {
  border-right-color: #666;
}

.metor_information .button-group--full-width {
  table-layout: fixed;
  width: 100%;
}


.metor_information .button-group + .button-group {
  margin-top: 10px;
}

.metor_information .button-group-2{
	display: flex;
	flex-wrap: wrap;
	width: 100%;
}
.metor_information .button-group-2 .button-group__btn{
	margin-right: 15px;
}


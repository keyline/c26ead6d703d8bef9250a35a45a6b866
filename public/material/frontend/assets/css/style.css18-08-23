:focus{box-shadow: none !important;}
.bg-sky { background-color: #e0eff0; }
.bg-blue { background-color: #2799a3; }
.bg-black { background-color: var(--textColor); }
.flex-v-cen { display: flex; align-items: center; }
.flex-cen { display: flex; align-items: center; justify-content: center; }
.title-inner .title { color: #575757; font-size: 30px; font-weight: 500; font-family: 'roboto-regular', sans-serif; letter-spacing: 0.5px; text-align: center; text-transform: capitalize; }
.title-inner .title-content { color: #707070; font-size: 14px; font-weight: 500; font-family: 'roboto-regular', sans-serif; letter-spacing: 0.5px; text-align: center; margin: 10px 0px 20px 0px; }
.title-inner .title-img img { display: block; margin: 0 auto; }
.uppercase{text-transform:uppercase !important;}
.font600{font-weight: 600;}
.linkblue{color: #1a0dab;}
.modal-backdrop.show {
  opacity: .7;
}
*{margin: 0;padding: 0;}
